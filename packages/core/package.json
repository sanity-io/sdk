{
  "name": "@sanity/sdk",
  "version": "0.0.0-alpha.1",
  "private": false,
  "description": "Sanity SDK",
  "keywords": [
    "sanity",
    "sdk",
    "content operating system",
    "cms",
    "headless",
    "realtime",
    "content"
  ],
  "homepage": "https://github.com/sanity-io/sdk/tree/main/packages/core/README.md",
  "bugs": {
    "url": "https://github.com/sanity-io/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sdk.git"
  },
  "license": "MIT",
  "author": "Sanity <developers@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./_internal/cli": {
      "source": "./src/_exports/_internal/cli.ts",
      "import": "./dist/_internal/cli.js",
      "require": "./dist/_internal/cli.cjs",
      "default": "./dist/_internal/cli.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "_internal/cli": [
        "./dist/_internal/cli.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg build --strict --clean --check",
    "clean": "rimraf dist",
    "dev": "pkg watch",
    "docs": "typedoc --out docs --tsconfig ../../tsconfig.tsdoc.json",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "ts:check": "tsc --noEmit"
  },
  "lint-staged": {
    "!(*.{ts,tsx})": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": "@sanity/prettier-config",
  "dependencies": {
    "@sanity/client": "^6.22.3",
    "@sanity/schema": "^3.67.1",
    "@sanity/types": "^3.67.1",
    "@types/lodash-es": "^4.17.12",
    "debug": "^4.4.0",
    "form-data": "^4.0.1",
    "lodash-es": "^4.17.21",
    "reselect": "^5.1.1",
    "rxjs": "^7.8.1",
    "tar-fs": "^3.0.6",
    "yargs": "^17.7.2",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@repo/package.config": "workspace:*",
    "@sanity/cli": "^3.68.3",
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/prettier-config": "^1.0.3",
    "@sanity/types": "^3.68.3",
    "@types/debug": "^4.1.12",
    "@types/tar-fs": "^2.0.4",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "^9.17.0",
    "groq-js": "^1.14.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
