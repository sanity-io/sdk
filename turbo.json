{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DEV",
    "VISUALIZER",
    "PKG_VERSION",
    "NPM_CONFIG_PROVENANCE",
    "CI",
    "SDK_E2E_PROJECT_ID",
    "SDK_E2E_ORGANIZATION_ID",
    "SDK_E2E_DATASET_0",
    "SDK_E2E_DATASET_1",
    "SDK_E2E_SESSION_TOKEN",
    "SDK_E2E_USER_ID",
    "SDK_E2E_USER_PASSWORD",
    "RECAPTCHA_E2E_STAGING_KEY"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:bundle": {
      "dependsOn": ["^build:bundle"],
      "outputs": ["lib/**"]
    },
    "build:docs": {
      "dependsOn": ["^build"],
      "outputs": ["docs/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "clean": {},
    "cleanup": {
      "cache": false,
      "env": [
        "SDK_E2E_PROJECT_ID",
        "SDK_E2E_DATASET_0",
        "SDK_E2E_DATASET_1",
        "SDK_E2E_SESSION_TOKEN"
      ]
    },
    "depcheck": {
      "env": ["DEPCHECK"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "docs": {
      "dependsOn": ["^build"],
      "outputs": ["docs/**"]
    },
    "dev:e2e": {
      "persistent": true,
      "cache": false,
      "env": ["SDK_E2E_PROJECT_ID", "SDK_E2E_DATASET_0", "SDK_E2E_DATASET_1"]
    },
    "lint": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:coverage": {
      "outputs": ["coverage/**"]
    },
    "test:e2e": {
      "cache": false,
      "env": [
        "SDK_E2E_PROJECT_ID",
        "SDK_E2E_ORGANIZATION_ID",
        "SDK_E2E_DATASET_0",
        "SDK_E2E_DATASET_1",
        "SDK_E2E_SESSION_TOKEN",
        "NODE_ENV",
        "SDK_E2E_USER_ID",
        "SDK_E2E_USER_PASSWORD",
        "RECAPTCHA_E2E_STAGING_KEY"
      ],
      "dependsOn": ["@repo/e2e#build"],
      "outputs": ["e2e/test-results/**", "e2e/test-report/**"]
    },
    "@repo/e2e#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "deploy": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": ["SANITY_AUTH_TOKEN"]
    }
  }
}
