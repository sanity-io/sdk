{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>sanity-io/renovate-config",
    ":dependencyDashboardApproval"
  ],
  "ignorePresets": [
    "github>sanity-io/renovate-config:group-non-major"
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "groupName": "devDependencies (non-major)"
    }
  ]
}
