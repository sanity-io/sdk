{
  "name": "@sanity/kitchensink-react",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm tsc && vite build",
    "clean": "rimraf dist",
    "deploy": "sanity deploy",
    "dev": "sanity dev",
    "install": "npm run schema:extract && npm run types:generate",
    "lint": "eslint .",
    "paramour": "npx paramour --config=./src/css/css.config.js --output=./src/css/paramour.css",
    "preview": "vite preview",
    "schema:extract": "sanity schema extract --workspace ppsg7ml5-test",
    "test:e2e": "playwright test",
    "tsc": "tsc --noEmit",
    "types:generate": "npx sanity typegen generate"
  },
  "prettier": "@sanity/prettier-config",
  "dependencies": {
    "@paramour/css": "1.0.0-rc.2",
    "@sanity/icons": "^3.7.0",
    "@sanity/sdk-react": "workspace:*",
    "@sanity/ui": "^3.1.11",
    "groq": "3.88.1-typegen-experimental.0",
    "inter-ui": "^4.1.0",
    "json-edit-react": "^1.26.2",
    "react": "^19.2.1",
    "react-compiler-runtime": "19.1.0-rc.1",
    "react-dom": "^19.2.1",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.10.0",
    "sanity": "^4.20.0",
    "styled-components": "^6.1.18",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@repo/config-eslint": "workspace:*",
    "@repo/e2e": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@sanity/cli": "^4.20.0",
    "@sanity/prettier-config": "^1.0.3",
    "@sanity/sdk": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "vite": "^6.3.3",
    "vitest": "^3.2.4"
  }
}
